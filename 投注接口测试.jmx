	columnValue=vars.getObject(&quot;id&quot;).get(i).get(&quot;id&quot;);
	saveStringToTxt(&quot;/Users/admin/Desktop/id.txt&quot;,columnValue);
	//log.info(&quot;result:&quot;+columnValue);

